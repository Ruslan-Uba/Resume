<div class='row'>
  {{#each links}}
    <div class='col-2'>
      <div class='nav'>
        <div class='nav-item'>
          {{#if @last}}
            <a
              href='{{href}}'
              class='nav-link disabled'
            >{{text}}</a>
          {{else}}
            <a href='{{href}}' class='nav-link'>{{text}}</a>
          {{/if}}
        </div>
      </div>
    </div>
  {{/each}}
</div>