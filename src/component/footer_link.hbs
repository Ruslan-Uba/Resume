<div class='row m-5'>
  {{#each footer}}
    <div class='col-2'>
      {{#each this}}
        <div class='nav nav-flex-column'>
          <div class='nav-item'>
            {{#if @last}}
              <a
                href='{{link}}'
                class='nav-link disabled'
              >{{text}}</a>
            {{else}}
              <a
                href='{{link}}'
                class='nav-link'
              >{{text}}</a>
            {{/if}}
          </div>
        </div>
      {{/each}}
    </div>
  {{/each}}
</div>