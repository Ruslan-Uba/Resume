<main>
  <div class="mb-5">
    <div>
      {{#with heading}}      
        {{#with main}}
          <h1>Heading</h1>
          <p class="uppercase text-center">{{title}}</p>
          <p>{{text}}</p>
          {{#with button_text}}
            <div>         
              <button type="button" class="btn btn-primary mb-3">{{solid}}</button>       
            </div> 
            <div> 
              <button type="button" class="btn btn-outline-primary">{{outline}}</button>          
            </div> 
          {{/with}}
        {{/with}}
    </div>
    <div class="mt-5">
      {{#with second}}
        <h2>Heading second</h2>
        <p class="uppercase text-center">{{title}}</p>
        <p>{{text}}</p>
        {{#with button_text}}
          <div>         
            <button type="button" class="btn btn-secondary mb-3">{{solid}}</button>       
          </div> 
          <div> 
            <button type="button" class="btn btn-outline-secondary">{{outline}}</button>          
          </div> 
        {{/with}}
      {{/with}}
    </div>
    <div class="mt-5">
      {{#with subblock}}
        <h3>Heading subblock</h3>
        <p class="uppercase text-center">{{title}}</p>
        <p>{{text}}</p>
        {{#with button_text}}
          <div>         
            <button type="button" class="btn btn-dark mb-3">{{solid}}</button>       
          </div> 
          <div> 
            <button type="button" class="btn btn-outline-dark">{{outline}}</button>          
          </div> 
        {{/with}}
      {{/with}}
    </div>
    {{/with}}
  </div>
  <hr>
  <div class="mt-5">
    {{#with nested_display}}
    <p class="fs-1">{{title}}</p> 
    <div class="m-3">  
      {{#with children_display}}
        <p class="fs-4">{{title}}</p>
        <p>{{text}}</p>
        {{#with button}}
          <button type="button" class="btn btn-outline-warning mt-5 mb-3">{{text}}</button>
        {{/with}}
      {{/with}}
    </div>         
    <h6 class="mt-3">{{header_text}}</h6>
    {{#with button}}
      <button type="button" class="btn btn-info mt-3">{{text}}</button>
    {{/with}}    
    {{/with}}  
  </div>
  <hr>
  <div>
    <p class="text-uppercase">paragraph</p>
    {{#with paragraph}}
      <p class="text-lowercase">{{title}}</p>
      <p>{{text}}</p>
      {{#with button}}
        <p><a href="https://google.com" target="_blank">{{text}}</a></p>
      {{/with}}    
    <div class='m-5'>
      {{#each list_name}}
      <ul>
        <li><a href="{{url}}" target="_blank">{{text}}</a></li>
        {{!-- <li><a href="https://getbootstrap.com/">{{text}}</a></li> --}}
      </ul>
      {{/each}}
    </div>
    {{/with}}
  </div>
</main>